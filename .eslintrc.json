{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "settings": {
      "react": {
        "pragma": "React",
        "version": "15.5"
      }
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      },
      "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
